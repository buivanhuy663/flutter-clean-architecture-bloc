{
    "dart.flutterSdkPath": ".fvm/flutter_sdk",
    // Remove files from search
    "search.exclude": {
        "**/.fvm": true,
        "**/*.g.dart": true,
        "**/*.freezed.dart": true,
        "**/*.gr.dart": true,
        "**/custom_lint": false
    },
    // Remove from file watching
    "files.watcherExclude": {
        "**/.fvm": true,
    },
    "files.associations": {
        "*.arb": "json"
    },
    "files.insertFinalNewline": true,
    "cSpell.words": [
        "Autogenerated",
        "bools",
        "cirrusci",
        "cmdline",
        "cocoapods",
        "cupertino",
        "dartx",
        "ellipsize",
        "FASTLANE",
        "fullscreen",
        "genhtml",
        "interps",
        "KEYCHAIN",
        "lcov",
        "Podfile",
        "precache",
        "prefs",
        "previod",
        "pubspec",
        "rxdart",
        "screenutil",
        "sdkmanager",
        "serializable",
        "styfle",
        "subosito",
        "todos",
        "unawaited",
        "utsname",
        "xcodebuild",
        "XVCJ",
        "zgosalvez"
    ]
}
